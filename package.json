{
  "name": "nutri-view",
  "private": true,
  "version": "0.2.8",
  "author": "Viransh Shah",
  "type": "module",
  "scripts": {
    "prestart": "node sync.js",
    "dev": "concurrently \"npm run vue-dev\" \"npm run python-dev\"",
    "python-dev": "python -Xfrozen_modules=off -m debugpy --listen 5678 --wait-for-client backend/apppy.py",
    "vue-dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "npx electron . && vite",
    "electron:build": "vite build && electron-builder",
    "tauri": "node src-tauri/main.js && tauri",
    "tauri:build": "node src-tauri/main.js && tauri build && node src-tauri/move.js"
  },
  "dependencies": {
    "@hcaptcha/vue3-hcaptcha": "^1.3.0",
    "@tauri-apps/api": "^2.6.0",
    "@tauri-apps/cli": "^2.4.1",
    "@tauri-apps/plugin-dialog": "^2.2.1",
    "axios": "^1.10.0",
    "dom-to-image-more": "^3.6.0",
    "dompurify": "^3.2.6",
    "echarts": "^5.6.0",
    "esbuild": "^0.25.5",
    "json5": "^2.2.3",
    "leaflet": "^1.9.4",
    "leaflet.fullscreen": "^3.0.2",
    "lodash": "^4.17.21",
    "vue": "^3.5.17",
    "vue-echarts": "^7.0.3",
    "vue-multiselect": "^3.2.0",
    "vue-router": "^4.5.1",
    "vue-tree-list": "^1.5.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.17",
    "vite": "^6.3.4"
  }
}